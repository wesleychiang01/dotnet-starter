@model IEnumerable<WebApp_SolarIOT.Models.ParameterEntities>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.accuracy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.solar_volt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.solar_amp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.light_intensity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.temperature)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.battery_volt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.battery_amp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EventProcessedUtcTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.accuracy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.solar_volt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.solar_amp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.light_intensity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.temperature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.battery_volt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.battery_amp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EventProcessedUtcTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EventID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EventID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EventID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
